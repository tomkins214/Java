public class Multitables {
 
    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) { 
int max=12;
         for ( int i = 1; i <= max; i++ ) {
            for ( int j = 1; j <= max; j++ ) {
                System.out.print ( String.format ( "%4d", j * i ));
            }
            System.out.println();
        }

        // TODO code application logic here
    }
    
}
