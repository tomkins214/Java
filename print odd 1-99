public class Printoddnumbers {
     public static void printOdds() {
        for (int i = 1; i < 100; i += 2) {
            System.out.println ( i );
        }
    }

    /**
     * @param args the command line arguments
     */
    
    public static void main(String[] args) {
    
        printOdds();
        
    }
    
}
